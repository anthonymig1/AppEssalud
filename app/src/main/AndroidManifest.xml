<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.essalud.essalud">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ReferenceActivity"
            android:label="@string/title_activity_reference"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".view.LoginActivity" />
        <activity android:name=".view.AboutUsActivity" />
        <activity android:name=".view.LocationActivity" />
        <activity android:name=".view.AreasServicesActivity" />
        <activity android:name=".view.OurStaffActivity" />
        <activity android:name=".view.ReportResultsActivity"></activity>
    </application>

</manifest>